@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rml: <http://w3id.org/rml/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://schema.org/RMLFunctionExecutionShape> a sh:NodeShape ;
    sh:description """\r
    Represents a Function Execution.\r
    """ ;
    sh:name "rml:FunctionExecution" ;
    sh:property [ sh:description """\r
        Exactly one rml:function or rml:functionMap is required.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        Exactly one rml:function or rml:functionMap is required.\r
        """ ;
            sh:minCount 1 ;
            sh:name "rml:function/rml:functionMap" ;
            sh:path [ sh:alternativePath ( rml:function rml:functionMap ) ] ],
        [ sh:description """\r
        FnO function to execute.\r
        """ ;
            sh:message """\r
        rml:functionMap must be an IRI or Blank node.\r
        """ ;
            sh:name "rml:functionMap" ;
            sh:node <http://schema.org/RMLFunctionMap> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:functionMap ],
        [ sh:description """\r
        FnO function to execute.\r
        """ ;
            sh:message """\r
        rml:function must be an IRI.\r
        """ ;
            sh:name "rml:function" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:function ],
        [ sh:description """\r
        Input parameters for the function.\r
        """ ;
            sh:message """\r
        rml:input must be an IRI or Blank node.\r
        """ ;
            sh:name "rml:input" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:input ] ;
    sh:targetObjectsOf rml:functionExecution .

<http://schema.org/RMLFunctionMapShape> a sh:NodeShape ;
    sh:description """\r
    Represents a function map.\r
    """ ;
    sh:name "rml:FunctionMap" ;
    sh:property [ sh:description """\r
        A property for indicating whether a term map is a constant-valued term \r
        map.\r
        """ ;
            sh:message """\r
        rml:constant must be an IRI.\r
        """ ;
            sh:name "rml:constant" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:constant ],
        [ sh:description """\r
        An IRI indicating whether subject or object generated using the value \r
        from column name specified should be an IRI reference, blank node,\r
        or a Literal.\r
        """ ;
            sh:in ( rml:IRI ) ;
            sh:maxCount 1 ;
            sh:message """\r
        rml:termType must be either rml:IRI for an rmlf:ReturnMap.\r
        May only be provided once.\r
        """ ;
            sh:name "rml:termType" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:termType ],
        [ sh:description """\r
        A logical target is any target to where generated RDF triples are\r
        exported to.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        Zero or one rml:logicalTarget is required to export RDF triples.\r
        """ ;
            sh:minCount 0 ;
            sh:name "rml:logicalTarget" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:logicalTarget ] ;
    sh:targetObjectsOf rml:functionMap .

<http://schema.org/RMLInputShape> a sh:NodeShape ;
    sh:description """\r
    Represents an Input for a function.\r
    """ ;
    sh:name "rml:Input" ;
    sh:property [ sh:description """\r
        Exactly one rml:parameter or rml:parameterMap is required.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        Exactly one rml:parameter or rml:parameterMap is required.\r
        """ ;
            sh:minCount 1 ;
            sh:name "rml:parameter/rml:parameterMap" ;
            sh:path [ sh:alternativePath ( rml:parameter rml:parameterMap ) ] ],
        [ sh:description """\r
        Values of the parameters.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        rml:inputValueMap must be an IRI or Blank node, provided once.\r
        """ ;
            sh:name "rml:inputValueMap" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:inputValueMap ],
        [ sh:datatype xsd:string ;
            sh:description """\r
        Parameters to supply to FnO function.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        rml:inputValue must be a string Literal.\r
        """ ;
            sh:name "rml:inputValue" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:inputValue ],
        [ sh:description """\r
        Parameters to supply to FnO function.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        rml:parameterMap must be an IRI or Blank node.\r
        """ ;
            sh:name "rml:parameterMap" ;
            sh:node <http://schema.org/RMLParameterMap> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:parameterMap ],
        [ sh:description """\r
        Parameters to supply to FnO function.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        rml:parameter must be an IRI.\r
        """ ;
            sh:name "rml:parameter" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:parameter ],
        [ sh:description """\r
        Exactly one rml:inputValue or rml:inputValueMap is required.\r
        """ ;
            sh:maxCount 1 ;
            sh:message """\r
        Exactly one rml:inputValue or rml:inputValueMap is required.\r
        """ ;
            sh:minCount 1 ;
            sh:name "rml:inputValue/rml:inputValueMap" ;
            sh:path [ sh:alternativePath ( rml:inputValue rml:inputValueMap ) ] ] ;
    sh:targetObjectsOf rml:input .

